        <record id="black_solar_type" model="solar.type">
            <field name="name">Black</field>
        </record>
        <record id="ulta_black_solar_type" model="solar.type">
            <field name="name">Ultra Black</field>
        </record>
        <record id="cristal_solar_type" model="solar.type">
            <field name="name">Cristal</field>
        </record>
        <record id="premium_solar_type" model="solar.type">
            <field name="name">Premium</field>
        </record>
        <record id="formulier_custom_type_3" model="formulier.customer.type">
            <field name="name">Intake PV</field>
            <field name="technical_name">formulier_three</field>
        </record>
    </data>
</odoo>